<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.materialdesignexample" >

    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".OtherActivity"></activity>
        <activity android:name=".activities.RepositoriesListActivity"></activity>
        <activity android:name=".ClipViewsActivity"></activity>
        <activity android:name=".RecyclerViewActivity"></activity>
        <activity android:name=".activities.RepositoriesActivity"></activity>
        <activity android:name=".activities.DetailsRepositoriesActivity"></activity>
        <activity android:name=".activities.RepositoryDetailsActivity"></activity>

    </application>

</manifest>
